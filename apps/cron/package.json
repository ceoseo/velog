  "engines": {
    "node": ">=20.11.1"
  },
  "main": "/src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch './**/*.mts' --exec 'node --import @swc-node/register/esm-register' ./src/main.mts | pino-pretty",
    "start": "NODE_ENV=production node dist/src/main.mjs",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "build:prod": "tsc --project tsconfig.prod.json && tsc-alias -p tsconfig.prod.json",
    "lint": "prettier -w .",
    "create-service": "tsx ./scripts/createService.ts",
    "ssm": "tsx ./scripts/ssm.mts"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@packages/database": "workspace:*",
    "@packages/scripts": "workspace:*",
    "@prisma/client": "^5.11.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.26.2",
    "fastify-cron": "^1.3.1",
    "fastify-plugin": "^4.5.1",
    "geoip-country": "^4.2.68",
    "inquirer": "^9.2.7",
    "ioredis": "^5.3.2",
    "pino-pretty": "^10.0.0",
    "prisma": "^5.11.0",
    "reflect-metadata": "^0.1.13",
    "tsc-alias": "^1.8.7",
    "tsx": "^4.7.1",
    "tsyringe": "^4.7.0",
    "typescript": "^5.3.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@swc-node/register": "^1.9.0",
    "@types/geoip-country": "^4.0.2",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^2.0.22",
    "prettier": "^3.2.5",
    "ts-paths-esm-loader": "^1.4.3"
  }
