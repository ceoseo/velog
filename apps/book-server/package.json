{
  "name": "book-server",
  "version": "1.0.0",
  "description": "",
  "author": {
    "name": "velopert",
    "email": "public.velopert@gmail.com"
  },
  "main": "./src/main.mts",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch './**/*.mts' --exec 'node --import @swc-node/register/esm-register' ./src/main.mts | pino-pretty",
    "start": "node --import @swc-node/register/esm-register dist/src/main.mjs",
    "build": "tsc --project tsconfig.json",
    "codegen": "graphql-codegen-esm --config codegen.mts",
    "create-mock": "NODE_ENV=development dotenv -e ./env/.env.development -- tsx ./scripts/createMock.mts",
    "create-service": "tsx ./scripts/createService.mts",
    "prisma:copy": "tsx ./scripts/copyPrisma.mts",
    "prisma:generate": "prisma generate --schema='./prisma/velog-book-mongo/schema.prisma'",
    "ssm": "tsx ./scripts/ssm.mts"
  },
  "keywords": [
    "velog",
    "velog-book"
  ],
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.525.0",
    "@fastify/autoload": "^5.8.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/mongodb": "^8.0.0",
    "@graphql-tools/graphql-file-loader": "^8.0.0",
    "@graphql-tools/load": "^8.0.0",
    "@graphql-tools/merge": "^9.0.0",
    "@packages/database": "workspace:*",
    "@prisma/client": "^5.11.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "graphql": "^16.7.1",
    "graphql-scalars": "^1.22.2",
    "ioredis": "^5.3.2",
    "mercurius": "^14.0.0",
    "mqemitter-redis": "^5.0.0",
    "prisma": "^5.11.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@packages/database": "workspace:*",
    "@packages/common-scripts": "workspace:*",
    "@faker-js/faker": "^8.3.1",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.0",
    "@swc-node/register": "^1.9.0",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.2",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "inquirer": "^9.2.15",
    "jest": "^29.6.4",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
