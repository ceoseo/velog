{
  "name": "velog-book-server",
  "version": "1.0.0",
  "description": "",
  "main": "./src/main.mts",
  "module": "type",
  "scripts": {
    "dev": "nodemon --watch './**/*.mts' --exec 'node --import @bleed-believer/path-alias' ./src/main.mts | pino-pretty",
    "start": "node --import @bleed-believer/path-alias dist/src/main.mjs",
    "build": "tsc --project tsconfig.json",
    "codegen": "graphql-codegen-esm --config codegen.mts",
    "create-service": "tsx ./scripts/createService.mts"
  },
  "keywords": [
    "velog",
    "velog-book"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.525.0",
    "@bleed-believer/path-alias": "^1.1.2",
    "@fastify/autoload": "^5.7.1",
    "@fastify/cors": "^8.3.0",
    "@fastify/mongodb": "^8.0.0",
    "@graphql-tools/graphql-file-loader": "^8.0.0",
    "@graphql-tools/load": "^8.0.0",
    "@graphql-tools/merge": "^9.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.18.0",
    "graphql": "^16.7.1",
    "graphql-scalars": "^1.22.2",
    "mercurius": "^13.0.0",
    "mongodb": "^6.4.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.0",
    "@swc/core": "^1.4.2",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.24",
    "inquirer": "^9.2.15",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
