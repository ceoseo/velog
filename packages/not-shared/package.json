    "date-fns": "^2.30.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.12",
    "ioredis": "^5.3.2",
    "prisma": "^5.11.0",
    "reflect-metadata": "^0.1.13",
    "tsx": "^4.7.1",
    "tsyringe": "^4.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@swc-node/register": "^1.9.0",
