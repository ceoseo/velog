{
  "name": "velog-cron",
  "version": "1.0.0",
  "author": {
    "name": "velopert",
    "email": "public.velopert@gmail.com"
  },
  "description": "",
  "keywords": [
    "velog",
    "cron"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.16"
  },
  "main": "/src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch './**/*.ts' --exec 'node --loader ts-paths-esm-loader' src/main.ts | pino-pretty",
    "stage": "pnpm ssm pull -e stage && DOCKER_ENV=stage pnpm start",
    "prod": "pnpm ssm pull -e production && DOCKER_ENV=production pnpm start",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/src/main.js",
    "prisma:copy": "tsx ./scripts/copyPrisma.ts",
    "prisma:rm": "rm -rf ./prisma",
    "create-service": "tsx ./scripts/createService.ts",
    "ssm": "tsx ./scripts/ssm/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.379.1",
    "@fastify/autoload": "^5.7.1",
    "@fastify/cors": "^8.3.0",
    "@prisma/client": "^5.8.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.18.0",
    "fastify-cron": "^1.3.1",
    "fastify-plugin": "^4.5.1",
    "inquirer": "^9.2.7",
    "ioredis": "^5.3.2",
    "pino-pretty": "^10.0.0",
    "prisma": "^5.8.1",
    "reflect-metadata": "^0.1.13",
    "tsc-alias": "^1.8.7",
    "tsx": "^4.6.2",
    "tsyringe": "^4.7.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.2",
    "ts-paths-esm-loader": "^1.4.3",
    "typescript": "^5.3.3"
  }
}
